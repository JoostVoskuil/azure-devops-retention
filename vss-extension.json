{
    "manifestVersion": 1,
    "id": "retention",
    "name": "Retain Release and Pipelines (incl. cascade)",
    "publisher": "foxholenl",
    "description": "This extension makes it possible to retain pipelines and releases (and cascading back to pipelines).",
    "version": "1.0.0",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "README.MD"
        }
    },
    "icons": {
        "default": "images/logo.png"
    },
    "links": {
        "support": {
            "uri": "https://github.com/JoostVoskuil/azure-devops-retention"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/JoostVoskuil/azure-devops-retention"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "foxholenl-retention",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}